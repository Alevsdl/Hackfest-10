{
  "name": "eslint",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/node": "20.2.6",
    "@types/react": "18.2.11",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "eslint": "^8.42.0",
    "eslint-config-next": "13.4.4",
    "next": "13.4.4",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "eslint-plugin-tailwindcss": "^3.12.1",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0"
  },
  "lint-staged": {
    "*.js": "yarn lint",
    "*.{js,css,md*}": "yarn format"
  }
}
